{
	"name": "bramdeleeuw/norhagewebshop",
	"authors": [
		{
			"name": "bram de leeuw",
			"email": "bramdeleeuw@gmail.com"
		}
	],
	"require": {
		"johnpbloch/wordpress": "^6.3",
		"wpengine/advanced-custom-fields-pro": "6.2.1.1",
		"wpackagist-plugin/wordpress-seo": "^21.4",
		"wpackagist-plugin/post-types-order": "^2.1",
		"wpackagist-plugin/acf-content-analysis-for-yoast-seo": "^3.1",
		"wpackagist-plugin/woocommerce": "^8.5",
		"wpackagist-plugin/woocommerce-pdf-invoices-packing-slips": "^3.7",
		"wpackagist-plugin/contact-form-7": "^5.8",
		"wpackagist-plugin/webp-express": "^0.25.8",
		"wpackagist-plugin/svea-checkout-for-woocommerce": "^2.6",
		"wpackagist-plugin/redirection": "^5.4",
		"wpackagist-plugin/woocommerce-ajax-filters": "1.6.4.6",
		"wpackagist-plugin/insta-gallery": "^4.4",
		"wpackagist-plugin/klarna-checkout-for-woocommerce": "^2.12",
		"wpackagist-plugin/advanced-woo-labels": "^1.98",
		"wpackagist-plugin/cookie-law-info": "^3.2",
		"wpackagist-plugin/crisp": "^0.46.0",
		"wpackagist-plugin/wt-woocommerce-sequential-order-numbers": "^1.6",
		"wpackagist-plugin/duracelltomi-google-tag-manager": "^1.20",
		"wpackagist-plugin/wp-optimize": "^3.5",
		"brrrm/bulk-actions-for-multi-currency": "^0.2.0",
		"wpackagist-plugin/wp-super-cache": "^1.12",
		"wpackagist-plugin/w3-total-cache": "^2.7",
		"wpackagist-plugin/woo-cart-abandonment-recovery": "^1.3"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
	    	]
		},
		{
			"type": "composer",
			"url": "https://connect.advancedcustomfields.com"
		},
		{
			"type": "vcs",
			"url": "https://github.com/brrrm/bulk-actions-for-multi-currency.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/brrrm/garbo-translate-woocommerce-cart-abandonment-recovery.git"
		}
	],
	"extra": {
		"wordpress-install-dir": "public",
		"installer-paths": {
			"public/wp-content/mu-plugins/{$name}/": [
				"type:wordpress-muplugin"
			],
			"public/wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"public/wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	},
	"config": {
		"allow-plugins": {
			"johnpbloch/wordpress-core-installer": true,
			"composer/installers": true
		}
	}
}
